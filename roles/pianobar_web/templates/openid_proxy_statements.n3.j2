@prefix : <http://bigasterisk.com/openid_proxy#> .
@prefix site: <http://bigasterisk.com/openidProxySite/> .
@prefix acl: <http://www.w3.org/ns/auth/acl#> .

site:pianobar{{ansible_hostname.capitalize()}}
  :host "bigasterisk.com";
  :requestPrefix "/pianobar/{{ansible_hostname}}/";
  :proxyUrlPrefix "http://[fcbb:410f:4398:0161:ad3a:8305:1b29:700b]:8080/" .

:admin1 acl:accessTo site:pianobar{{ansible_hostname.capitalize()}} .
:admin2 acl:accessTo site:pianobar{{ansible_hostname.capitalize()}} .

      
